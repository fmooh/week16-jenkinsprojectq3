pipeline {
    agent any 
    tools {
        maven 'M2_HOME'
    }
    stages{
        stage('sonarqube scan'){
            steps{
                withSonarQubeEnv('sonarserver'){
                sh 'mvn verify org.sonarsource.scanner.maven:sonar-maven-plugin:sonar -Dsonar.projectKey=fmooh_week16-jenkinsprojectq3'
                  
            }
            }
        }
        stage('all maven commands'){
            steps{
                sh 'mvn clean compile install package'
            }
        }
        stage('upload artifact'){
            steps{
                sh 'curl -uadmin:AP2VJAxR5X9qYMhSDpg96sKVLPJ -T \
                target/bio*.jar \
                "http://52.91.104.236:8081/artifactory/geoapp/"'

            }
        }
    }
}